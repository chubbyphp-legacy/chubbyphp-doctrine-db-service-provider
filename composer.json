{
    "name": "chubbyphp/chubbyphp-doctrine-db-service-provider",
    "description": "Chubbyphp database service provider",
    "keywords": ["chubbyphp", "provider", "doctrine", "dbal", "orm", "database", "pimple"],
    "license": "MIT",
    "authors": [
        {
            "name": "Dominik Zogg",
            "email": "dominik.zogg@gmail.com"
        }
    ],
    "require": {
        "php": "^7.2",
        "doctrine/cache": "^1.6",
        "doctrine/common": "^2.9",
        "doctrine/dbal": "^2.9"
    },
    "conflict": {
        "doctrine/orm": "<2.6.3,>=3.0"
    },
    "require-dev": {
        "chubbyphp/chubbyphp-mock": "^1.4",
        "doctrine/orm": "^2.6.3",
        "friendsofphp/php-cs-fixer": "^2.15.3",
        "infection/infection": "^0.14.1",
        "nunomaduro/phpinsights": "^1.9",
        "phploc/phploc": "^5.0",
        "phpstan/phpstan": "^0.11.16",
        "phpunit/phpunit": "^8.3.5",
        "pimple/pimple": "^3.2.3",
        "psr/log": "^1.0",
        "symfony/yaml": "^2.8|^3.4|^4.2"
    },
    "autoload": {
        "psr-4": { "Chubbyphp\\DoctrineDbServiceProvider\\": "src/" }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.4-dev"
        }
    }
}
